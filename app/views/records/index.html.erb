<p id="notice"><%= notice %></p>

<h1>Listing Records</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Starts at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= month_calendar events: @records do |date, records| %>
  <%= date %>

  <% records.each do |record| %>
    <div>
      <%= record.name %>
    </div>
  <% end %>
<% end %>
    <%# @records.each do |record| %>
     
  </tbody>
</table>

<br>

<%#= link_to 'New Record', new_record_path %>
